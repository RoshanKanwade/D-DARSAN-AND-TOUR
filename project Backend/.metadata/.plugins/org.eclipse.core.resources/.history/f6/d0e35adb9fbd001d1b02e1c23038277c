import org.springframework.web.bind.annotation.RestController;

import com.app.pojos.BookingDetails;

@RestController
@RequestMapping("/auth")
public class BookingController {
	
	@GetMapping("/Booking")
	
	public String BookingHotel() {
		
		return BookingDetails
	}
	
	
	

}
